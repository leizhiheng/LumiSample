<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--blurring view-->
    <declare-styleable name="PxBlurringView">
        <attr name="blurRadius" format="integer"/>
        <attr name="downsampleFactor" format="integer"/>
        <attr name="overlayColor" format="color"/>
    </declare-styleable>

    <!-- 获取验证码按钮 -->
    <declare-styleable name="CircularProgressButton">
        <attr name="cpb_selectorIdle" format="reference" />
        <attr name="cpb_selectorComplete" format="reference" />
        <attr name="cpb_selectorError" format="reference" />
        <attr name="cpb_textComplete" format="string" />
        <attr name="cpb_textIdle" format="string" />
        <attr name="cpb_textError" format="string" />
        <attr name="cpb_textProgress" format="string" />
        <attr name="cpb_colorProgress" format="color" />
        <attr name="cpb_colorIndicator" format="color" />
        <attr name="cpb_colorIndicatorBackground" format="color" />
        <attr name="cpb_iconError" format="reference" />
        <attr name="cpb_iconComplete" format="reference" />
        <attr name="cpb_cornerRadius" format="dimension" />
        <attr name="cpb_paddingProgress" format="dimension" />
    </declare-styleable>

    <declare-styleable name="LoadingImageView">
        <attr name="colorProgress" format="color" />
    </declare-styleable>

    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="unit" format="string" />
        <attr name="unitSize" format="dimension"/>
        <attr name="textTypeface" format="string"/>
        <attr name="reverse" format="boolean" />
        <attr name="max" format="integer"></attr>
        <attr name="progress" format="integer"></attr>
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>


    <declare-styleable name="TitleBar">
        <attr name="type" format="enum">
            <enum name="center_text" value="1" />
            <enum name="center_tab" value="2" />
        </attr>
        <attr name="title_bar_background" format="color" />
        <attr name="iv_left_res_id" format="reference" />
        <attr name="iv_right_res_id" format="reference" />
        <attr name="iv_second_right_res_id" format="reference" />
        <attr name="text_left" format="string" />
        <attr name="text_left_color" format="color" />
        <attr name="text_left_size" format="dimension" />
        <attr name="text_right_color" format="color" />
        <attr name="text_right_size" format="dimension" />
        <attr name="text_right" format="string" />
        <attr name="text_center" format="string" />
        <attr name="text_center_color" format="color" />
        <attr name="text_center_size" format="dimension" />
        <attr name="tab_text_left" format="string" />
        <attr name="tab_text_right" format="string" />
        <attr name="show_shadow_line" format="boolean" />
        <attr name="text_center_drawable_right" format="reference" />
        <attr name="text_center_drawable_padding" format="dimension" />
    </declare-styleable>
    <declare-styleable name="CommonCell">
        <attr name="cell_type" format="enum">
            <enum name="cell_normal" value="1" />
            <enum name="cell_arrow" value="2" />
            <enum name="cell_switch" value="3" />
            <enum name="cell_checkbox" value="4" />
            <enum name="cell_seek_bar" value="5" />
        </attr>
        <attr name="iv_right_type" format="enum">
            <enum name="cell_checkbox" value="1" />
            <enum name="cell_switch" value="2" />
            <enum name="cell_arrow" value="3" />
        </attr>
        <attr name="tv_right_type" format="enum">
            <enum name="cell_stroke" value="1" />
            <enum name="cell_fill" value="2" />
            <enum name="cell_text" value="3" />
        </attr>
        <attr name="show_line_top" format="boolean" />
        <attr name="show_line_bottom" format="boolean" />
        <attr name="show_shadow_top" format="boolean" />
        <attr name="show_shadow_bottom" format="boolean" />
        <attr name="iv_right_selected" format="boolean" />
        <attr name="text_cell_left" format="string" />
        <attr name="text_cell_right" format="string" />
        <attr name="text_cell_left_bottom" format="string" />
        <attr name="text_cell_edit" format="string" />
        <attr name="text_cell_edit_hint" format="string" />
        <attr name="iv_cell_left_res_id" format="reference" />
        <attr name="cell_background" format="color" />
        <attr name="show_left_dot" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="WheelView">
        <attr name="wheel_view_gravity">
            <enum name="center" value="17"/>
            <enum name="left" value="3"/>
            <enum name="right" value="5"/>
        </attr>
        <attr name="wheel_view_text_size" format="dimension"/>
        <attr name="wheel_view_text_color_out" format="color"/>
        <attr name="wheel_view_text_color_center" format="color"/>
        <attr name="wheel_view_label_size" format="dimension"/>
        <attr name="wheel_view_divider_color" format="color"/>
        <attr name="wheel_view_line_spacing_multiplier" format="float"/>
        <attr name="wheel_view_content_margin" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="ClearableEditText">
        <attr format="color|reference" name="clearIconDrawable"/>
        <attr format="boolean" name="clearIconDrawWhenFocused"/>
    </declare-styleable>

    <declare-styleable name="custom_curtain_view_style">
        <attr name="curtain_width" format="dimension"/>
        <attr name="curtain_height" format="dimension"/>
        <attr name="curtain_gravity" format="integer"/>
        <attr name="curtain_note_count" format="integer"/>
        <attr name="curtain_note_width" format="dimension"/>
        <attr name="curtain_note_src" format="reference"/>
        <attr name="curtain_delta_src" format="reference"/>
        <attr name="note_count" format="integer"/>
    </declare-styleable>

    <declare-styleable name="CommonSeekBar">
        <attr name="thumb_size" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="PercentView">
        <attr name="percent_view_unit" format="string" />
        <attr name="percent_view_unitSize" format="dimension"/>
        <attr name="percent_view_text" format="string"/>
        <attr name="percent_view_textColor" format="color" />
        <attr name="percent_view_textSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SpinView">
        <attr name="spin_view_res_id" format="reference" />
        <attr name="spin_view_frame_count" format="integer" />
    </declare-styleable>
</resources>